{
  "env_vars": {
    "Meow": "ニャア"
  },
  "build_commands": {
    "1": {
      "name": "Hello, World!",
      "shell_cmd": "echo 'Hello, World!2'",
      "cwd": ".",
      "autoopen": true,
      "print_result": false
    },
    "2": {
      "name": "Kill test",
      "shell_cmd": "while true; do date +'%Y-%m-%d %H%M:%S'; sleep 1 ; done",
      "cwd": ".",
      "autoopen": true,
      "print_result": false
    },
    "3": {
      "name": "env_var test",
      "shell_cmd": "echo $Meow",
      "cwd": ".",
      "autoopen": true,
      "print_result": false
    },
    "4": {
      "name": "Args Test",
      "shell_cmd": "echo $ARG1",
      "cwd": ".",
      "autoopen": true,
      "print_result": false
    },
    "5": {
      "name": "Blacklist test (show popup)",
      "shell_cmd": "echo my test; exit 1",
      "cwd": ".",
      "autoopen_blacklist": [
        0
      ]
    },
    "6": {
      "name": "Blacklist test (dont show popup)",
      "shell_cmd": "echo my test; exit 1",
      "cwd": ".",
      "autoopen_blacklist": [
        1
      ]
    },
    "7": {
      "name": "Whitelist test (show popup)",
      "shell_cmd": "echo my test; exit 1",
      "cwd": ".",
      "autoopen_whitelist": [
        1
      ]
    },
    "8": {
      "name": "Whitelist test (dont show popup)",
      "shell_cmd": "echo my test; exit 0",
      "cwd": ".",
      "autoopen_whitelist": [
        1
      ]
    },
    "9": {
      "name": "Mandatory args test",
      "shell_cmd": "echo $ARG1; exit 0",
      "cwd": ".",
      "prompt": "This guy wants some extra commands!",
      "min_args": 2,
      "autoopen": true
    },
    "named_test": {
      "name": "This is a named guy",
      "shell_cmd": "echo my test; exit 0",
      "cwd": "."
    }
  }
}
